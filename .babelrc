{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "^[^/]+": "./\\0"
        }
      }
    ],
    [
      "wrap-in-js",
      {
        "extensions": ["css$"]
      }
    ],
    [
      "emotion",
      {
        "inline": true
      }
    ]
  ]
}
